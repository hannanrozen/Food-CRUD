generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


enum FoodType {
  uph
  fresh
}

model Food {
  id          String   @id @default(cuid())
  name        String
  ingredients String
  description String   @db.Text
  type        FoodType
  imageUrl    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("foods")
}